<h1>New employee:</h1>
<ul>
  <%= form_for @employee do |f| %>
    <div class="">
    <% @attributes.each do |param, display| %>
      <li><%= f.label param, display %>
      <%= f.text_field param %>
    </li>
    <% end %></div>
    <div class="">
    <%= f.label :dog_id, "Dog" %>
    <%= f.collection_select :dog_id, @dogs, :id, :name %> </div>
    <br>
    <%= f.submit %>
  <% end %>
</ul>
